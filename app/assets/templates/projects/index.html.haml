.projects

  %a.pull-right{ class: "btn btn-small btn-primary", 'href' => "/github_repos" }
    Add new Project

  %ul.projects-list.list-group

    %li.project.list-group-item{ 'ng-repeat' => "project in projects" }
      %a{ 'href' => "{{project.url}}" }
        {{project.name}}

      %br
      %small {{project.description}}
